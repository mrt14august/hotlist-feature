{
  "name": "mylist-ott-backend",
  "version": "1.0.0",
  "description": "My List feature for OTT Platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "dev-ts": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "ts-node src/scripts/seed.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "OTT",
    "MyList",
    "Movies",
    "TVShows"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "autocannon": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "minimist": "^1.2.8",
    "mongoose": "^7.5.0",
    "redis": "^4.6.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
